                  },
                  "verify": {
                    "type": "boolean"
                  },
                  "_id": {
                    "type": "string"
                  },
                  "email": {
                    "type": "string",
                    "formate": "email"
                  },
                  "password": {
                    "type": "string"
                  },
                  "token": {
                    "type": "string"
                  },
                  "avatarURL": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "place": {
                    "type": "string"
                  },
                  "phone": {
                    "type": "string",
