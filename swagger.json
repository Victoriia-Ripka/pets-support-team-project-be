              "email": {
                "type": "string",
                "format": "email"
              },
              "avatarURL": {
                "type": "string"
              },
              "dateofbirth": {
                "type": "date",
                "format": "dd.mm.yyyy"
              },
              "favorites": {
                "type": "array",
                "items": {
