                    "type": "string"
                  },
                  "__v": {
                    "type": "integer"
                  }
                }
              },
              "pets": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "_id": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "date": {
                      "type": "string",
                      "format": "dd.mm.yyyy"
                    },
                    "breed": {
                      "type": "string"
                    },
                    "comment": {
                      "type": "string"
                    },
                    "owner": {
                      "type": "string",
                      "description": "User's unique id"
                    },
                    "createdAt": {
                      "type": "date",
                      "format": "date"
                    },
                    "updatedAt": {
                      "type": "date",
                      "format": "date"
                    }
                  }
                }
              }
            }
          }
